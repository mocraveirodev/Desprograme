algoritmo "Exercicio 5"
var
nome_aluno:caracter
p1, p2, p3, media:real

inicio
escreva("Informa o nome do aluno: ")
leia(nome_aluno)
escreva("Digita e primeira nota: ")
leia(p1)
escreva("Digita e segunda nota: ")
leia(p2)
escreva("Digita e terceira nota: ")
leia(p3)
media := (p1 + p1 + p3) / 3
escreva("O aluno ", nome_aluno, " teve média ", media)
fimalgoritmo
