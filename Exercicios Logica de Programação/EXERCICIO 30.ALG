algoritmo "Exercicio 30"
var
v, emp, d, p: inteiro
time: caracter
inicio
escreva("Informe o nome do time: ")
leia(time)
escreva("Informa o número de vitórias: ")
leia(v)
escreva("Informa o número de empates: ")
leia(emp)
escreva("Informa o número de derrotas: ")
leia(d)
p<-(v*3)+(emp*1)+(d*0)
escreva(p, " pontos obtidos no campeonato.")
fimalgoritmo
