Algoritmo "Exercicio 3"
Var
dist_total, comb_gasto, cons_medio: real

Inicio
escreva("Digite a distância percorrida: ")
leia(dist_total)
escreva("Digite o total de combustível gasto: ")
leia(comb_gasto)
cons_medio := dist_total/comb_gasto
escreva("O consumo médio do automóvel é: ", cons_medio, " km/l.")


Fimalgoritmo